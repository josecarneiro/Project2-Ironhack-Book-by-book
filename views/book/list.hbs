
{{>searchBar}}


<div class='style-body-pages'>
 <ul>
{{#each books}}
<div>
  <img src="{{picture}}" alt="{{title}}">
</div>
<div>
 <a href="book/{{_id}}">
  <h3>{{bookTitle}}</h3>
  <h4>{{author}}</h4>
  <p>{{booksComment}}</p>
 </a>
 <h6><a href="/profile/{{userCreator}}"> {{userCreator.name}} Profile</a></h6>
 <h6><a href="/profile/{{userCreator}}/books">Other books from this user</a></h6>
  
</div>
{{/each}}

</ul>
</div>  



